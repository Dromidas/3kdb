};

#NOP -- Buy White Capsule On Sight;
#action {^[Auction House]%*\n%a!\)$} {
    #var auctionPrice {%2};
    #replace auctionPrice {\n} { };
    #replace auctionPrice {%+2..s} { };
    #regex {$auctionPrice} {buy it now for %d} {#var auctionPrice &1};

    #var auctionItem {%1};
    #regex {$auctionItem} {a bright white capsule} {
        #if {$auctionPrice <= 1500000} {
            #echo {BUYING CAPSULE};
            ahlist;
            #line oneshot #action {%d a bright white capsu%*} {#var auctionNumber %%1;ahbid $auctionNumber $auctionPrice};
        };
    };
} {1};


