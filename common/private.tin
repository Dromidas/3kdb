#NOP -- Iprime Stuff;
#if {"$guild" == "necromancer"} {
    #var stats[invoke-prime][success] 0;
    #var stats[invoke-prime][failure] 0;
    #var stats[invoke-prime][total] 0;

    #action {%sThe %* is already infected with rabies.} {
        #var ip[mob_name] %2;
        #replace ip[mob_name] {-} {_};
    };

    #alias {ip1} {    
        #if {$corpses[coffin] > 0} {
            ipc;
        } {
            #if {$corpses[freezer] > 0} {
                ipf;
            } {
                #if {$corpses[cooler] > 0} {
                    ipcl;
                } {
                    #if {$corpses[inventory] > 0} {
                        iprime power;
                    };
                };
            };
        };
    };
    #alias {ipf} {deslab;preserve remains;get preservation;iprime power};
    #alias {ipc} {unwrap biggest;preserve remains;get preservation;iprime power};
    #alias {ipcl} {uncooler corpse;preserve remains;get preservation;iprime power};

    #alias iprime {
        #read common/guilds/necromancer/ips.tin;
        #var ip_secret_word !!!;
        
        #unvar scrabble;
        #NOP -- pppappp is what we'll use to match 'a';
        #list scrabble create {pppappp} {Seething} {venomous} {anger} {deaths} {head} {howls} {Diving}{around}{terrifying}{deeply}{Screaming} {in}{rage}{at}{skull} {room} {through}{growls}{Snarling} {gnashes} {its} {teeth} {The} {ghostly} {bobs} {and} {weaves} {growling} {fiercely} {races} {circling} {menacingly} {dives} {quickly} {towards} {but} {narrowly} {misses} {With} {a} {horrid} {grin} {of} {death} {leers}{pure} {hatred} {bares} {fangs} {hisses} {You} {call} {forth} {word} {great} {power} {invoking} {prime} {nature} {your} {magic} {Crumbling} {away} {from} {corpse} {soul} {creature} {is} {released} {form} {giant} {that} {flies} {freely} {into} {leaving} {behind} {it} {trail} {waves};

        #if $vaf[numbers] {
            #var temp $enemy[name_numbers]
        } {
            #format tempLength {%L} {${ip[mob_name]}};
            #if {$tempLength} {#var temp $ip[mob_name]} {#format {temp} {%n} {$enemy[target]}};
        };
        #replace temp {-} {_};
        #replace temp { } {;};
        #list scrabble add $temp;
        
        #delay 1 {invoke prime %1};
        #math my[necromancer][invoke-prime] {$my[necromancer][invoke-prime] - 1};
    };
};



